  "assessment_frameworks": {
    "bloom_taxonomy": {
      "levels": [
        {
          "level": 1,
          "name": "Remember",
          "description": "Recall facts and basic concepts",
          "verbs": ["define", "list", "describe", "identify", "state", "name"]
        },
        {
          "level": 2,
          "name": "Understand",
          "description": "Explain ideas or concepts",
          "verbs": ["explain", "summarize", "interpret", "classify", "compare", "describe"]
        },
        {
          "level": 3,
          "name": "Apply",
          "description": "Use information in new situations",
          "verbs": ["implement", "execute", "solve", "use", "apply", "demonstrate"]
        },
        {
          "level": 4,
          "name": "Analyze",
          "description": "Draw connections among ideas",
          "verbs": ["differentiate", "organize", "attribute", "compare", "contrast", "examine"]
        },
        {
          "level": 5,
          "name": "Evaluate",
          "description": "Justify a decision or course of action",
          "verbs": ["check", "critique", "judge", "test", "detect", "monitor", "assess"]
        },
        {
          "level": 6,
          "name": "Create",
          "description": "Produce new or original work",
          "verbs": ["design", "construct", "develop", "formulate", "create", "produce"]
        }
      ]
    },
    "rubric_types": {
      "analytical": {
        "description": "Break down assessment into multiple criteria",
        "structure": {
          "criteria": "Each criterion is scored separately",
